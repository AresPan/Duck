<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Shape">
        <attr name="solid" format="reference|color" />
        <attr name="corner" format="dimension" />
        <attr name="corner_top_left" format="dimension" />
        <attr name="corner_top_right" format="dimension" />
        <attr name="corner_bottom_left" format="dimension" />
        <attr name="corner_bottom_right" format="dimension" />
        <attr name="stroke_width" format="dimension" />
        <attr name="stroke_color" format="color" />
        <attr name="stroke_dash_gap" format="dimension" />
        <attr name="stroke_dash_width" format="dimension" />
        <attr name="shape">
            <flag name="rectangle" value="0" />
            <flag name="oval" value="1" />
            <flag name="line" value="2" />
            <flag name="right" value="3" />
        </attr>
    </declare-styleable>


