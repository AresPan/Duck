<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MyView">
        <attr format="reference|color" name="solid"/>
        <attr format="dimension" name="corner"/>
        <attr format="dimension" name="corner_top_left"/>
        <attr format="dimension" name="corner_top_right"/>
        <attr format="dimension" name="corner_bottom_left"/>
        <attr format="dimension" name="corner_bottom_right"/>
        <attr format="dimension" name="stroke_width"/>
        <attr format="color" name="stroke_color"/>
        <attr format="dimension" name="stroke_dash_gap"/>
        <attr format="dimension" name="stroke_dash_width"/>
        <attr format="boolean" name="focusable"/>
        <attr name="shape">
            <flag name="rectangle" value="0"/>
            <flag name="oval" value="1"/>
            <flag name="line" value="2"/>
            <flag name="right" value="3"/>
        </attr>
    </declare-styleable>


